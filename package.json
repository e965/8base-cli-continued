{
  "name": "8base-cli-continued",
  "version": "1.0.0-beta.16",
  "description": "8base CLI",
  "author": "8base",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "8base": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/trakservices/8base-cli-continued.git"
  },
  "files": [
    "dist",
    "scripts",
    "templates"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup --config rollup.config.mjs",
    "watch": "npm run build -- --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint --ext js,ts src",
    "lint:fix": "npm run lint --fix",
    "prettier:check": "prettier --list-different \"{src,e2e}/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"{src,e2e}/**/*.{ts,tsx}\"",
    "e2e": "vitest run --config e2e/vitest.config.ts",
    "e2e:watch": "vitest --config e2e/vitest.config.ts",
    "e2e:update-snapshot": "vitest run --config e2e/vitest.config.ts --update",
    "upload": "npm version patch && git push --follow-tags",
    "postinstall": "node ./scripts/is-ci.cjs || (node ./scripts/postinstall.js && (test -e dist/index.js && node dist/index.js --help || echo))"
  },
  "dependencies": {
    "@8base/api-client": "^3.0.0-beta.3",
    "@8base/error-codes": "^1.0.23",
    "@8base/schema-name-generator": "^0.1.23",
    "@paralleldrive/cuid2": "^2.2.2",
    "adm-zip": "^0.5.10",
    "archiver": "^7.0.1",
    "chalk": "^4.1.2",
    "ci-info": "^4.0.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "fs-extra": "^11.2.0",
    "graphql": "^16.8.1",
    "graphql-request": "^7.0.1",
    "graphql-tag": "^2.12.6",
    "i18next": "^23.7.16",
    "ignore": "^5.3.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "memorystream": "^0.3.1",
    "open": "^8.4.2",
    "ora": "^5.4.1",
    "prompts": "^2.4.2",
    "simple-git": "^3.22.0",
    "table": "^6.8.1",
    "tree-node-cli": "^1.6.0",
    "typescript": "^5.3.3",
    "validate-npm-package-name": "^5.0.0",
    "winston": "^3.11.0",
    "yaml": "^2.3.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/adm-zip": "^0.5.5",
    "@types/archiver": "^6.0.2",
    "@types/ejs": "^3.1.5",
    "@types/eslint": "^8.56.10",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.4",
    "@types/memorystream": "^0.3.4",
    "@types/node": "^20.12.13",
    "@types/prompts": "^2.4.9",
    "@types/ramda": "^0.30.0",
    "@types/validate-npm-package-name": "^4.0.2",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.5.4",
    "execa": "^9.1.0",
    "fast-glob": "^3.3.2",
    "prettier": "^3.2.5",
    "rollup": "^4.18.0",
    "strip-ansi": "^7.1.0",
    "vitest": "^1.6.0"
  },
  "overrides": {
    "@8base/api-client": {
      "filestack-js": "^3.29.0"
    }
  }
}
